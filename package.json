{
  "name": "imageboard",
  "version": "0.5.0",
  "description": "An easy uniform wrapper over the popular imageboards' API",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./DOMParser": {
      "node": "./DOMParser/node.js",
      "default": "./DOMParser/browser.js"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@xmldom/xmldom": "^0.8.2",
    "social-components": "^0.2.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "copyfiles": "^2.1.1",
    "mocha": "^10.0.0",
    "node-fetch": "^3.2.4",
    "npm-run-all": "^1.4.0"
  },
  "scripts": {
    "test-chan": "node --experimental-json-modules test/test.js",
    "test": "node --experimental-json-modules node_modules/mocha/bin/_mocha --require ./test/setup.js --reporter spec --colors --bail --exit --recursive \"./{,!(node_modules)/**/}*.test.js\"",
    "copy-chan-settings": "cd source && copyfiles chan/**/*.json ../modules && copyfiles chan/**/*.json ../commonjs",
    "build": "npm run copy-chan-settings",
    "prepublishOnly": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/catamphetamine/imageboard.git"
  },
  "keywords": [
    "imageboard",
    "4chan",
    "8ch",
    "chan"
  ],
  "author": "catamphetamine <purecatamphetamine@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/catamphetamine/imageboard/issues"
  },
  "homepage": "https://gitlab.com/catamphetamine/imageboard#readme"
}
